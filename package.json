{
  "name": "web-platform-starter",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "description": "Meetup Web Starter Repo",
  "engine": "node >=6.4.0",
  "scripts": {
    "build:clean": "rm -rf build/",
    "build:locales": "node scripts/build-server-app",
    "build:server": "node scripts/build-server",
    "clean": "yarn run build:clean && rm *.log || true",
    "coverage": "yarn run test:unit -- --coverage",
    "coveralls": "if [ \"${TRAVIS_PULL_REQUEST}\" = \"false\" ] && [ \"${COVERALLS_REPO_TOKEN}\" != \"\" ]; then cat coverage/lcov.info | coveralls; fi",
    "debug:server": "yarn run build:server && node --inspect --debug-brk build/app-server.js",
    "generate": "node util/generate.js",
    "lint": "yarn run lint:base -- --fix",
    "lint:base": "eslint . --ext .js,.jsx",
    "modules:clean": "rm -rf node_modules",
    "precommit": "git status --porcelain | awk 'match($1, \"[ACM]\"){print $2}' | grep '[js|jsx]$' | xargs eslint",
    "package": "npm-run-all --parallel lint:base build:locales test:coverage --serial coveralls build:server test:integration",
    "start": "rm -f *.log && yarn run start:all -- en-US",
    "start:all": "npm-run-all stop build:clean --parallel \"build:locales -- {@}\" \"start:asset -- {@}\" --serial build:server start:app --",
    "start:app": "forever start scripts/forever/app-server.json && yarn run start:info",
    "start:asset": "node scripts/forever/asset-server.js",
    "start:info": "node scripts/start-info.js",
    "start:prod": "node scripts/runServer.js",
    "stop": "forever stopall",
    "tail": "tail -f ./*.log",
    "test": "npm-run-all --parallel lint:base test:unit",
    "test:coverage": "yarn run test:unit -- --coverage --runInBand",
    "test:unit": "jest --silent --config jest.config.json",
    "test:integration": "jest --config jest.int.config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetup/web-platform-starter.git"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "html-entities": "1.2.0",
    "inert": "4.1.0",
    "meetup-web-components": "0.1.205",
    "meetup-web-platform": "0.11.663",
    "node-fetch": "1.6.3",
    "react": "15.4.2",
    "react-helmet": "3.3.2",
    "react-intl": "^2.2.3",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "redux": "3.6.0",
    "rxjs": "5.0.3",
    "swarm-icons": "git+https://github.com/meetup/swarm-icons"
  },
  "devDependencies": {
    "ascii-progress": "1.0.5",
    "babel-cli": "6.22.1",
    "babel-core": "6.22.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "coveralls": "2.11.15",
    "css-loader": "0.26.1",
    "eslint": "3.13.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-meetup": "git+https://github.com/meetup/eslint-plugin-meetup.git",
    "eslint-plugin-react": "6.8.0",
    "file-loader": "0.9.0",
    "forever": "0.15.3",
    "gettext-parser": "1.2.2",
    "glob": "7.1.1",
    "husky": "0.12.0",
    "inquirer": "2.0.0",
    "jest-cli": "18.1.0",
    "meetup-web-mocks": "0.1.51",
    "mkdirp": "0.5.1",
    "node-sass": "4.3.0",
    "npm-run-all": "4.0.1",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "sass-loader": "4.1.1",
    "sassquatch2": "2.4.4",
    "style-loader": "0.13.1",
    "transifex": "git+https://github.com/mattkime/node-transifex.git#push_translations",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0",
    "webpack-node-externals": "1.5.4"
  }
}
