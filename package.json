{
  "name": "web-platform-starter",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "description": "Meetup Web Starter Repo",
  "engine": "node >=7.5.0",
  "scripts": {
    "build:clean": "rm -rf build/",
    "build:locales": "node scripts/build-server-app",
    "build:server": "node scripts/build-server",
    "clean": "yarn run build:clean && rm *.log || true",
    "coverage": "yarn run test:unit -- --coverage",
    "coveralls": "if [ \"${TRAVIS_PULL_REQUEST}\" = \"false\" ] && [ \"${COVERALLS_REPO_TOKEN}\" != \"\" ]; then cat coverage/lcov.info | coveralls; fi",
    "debug:server": "yarn run build:server && node --inspect --debug-brk build/app-server.js",
    "flow": "flow",
    "generate": "node util/generate.js",
    "lint": "yarn run lint:base -- --fix",
    "lint:base": "eslint . --ext .js,.jsx",
    "modules:clean": "rm -rf node_modules",
    "precommit": "lint-staged",
    "package": "npm-run-all --parallel lint:base build:locales test:coverage --serial build:server test:integration",
    "postcheckout": "yarn install",
    "postmerge": "yarn install",
    "start": "rm -f *.log && yarn run start:all -- en-US",
    "start:dev": "yarn build:clean && node scripts/start-dev",
    "start:all": "npm-run-all stop build:clean --parallel \"build:locales -- {@}\" \"start:asset -- {@}\" --serial build:server start:app --",
    "start:app": "forever start scripts/forever/app-server.json && yarn run start:info",
    "start:asset": "node scripts/forever/asset-server.js",
    "start:info": "node scripts/start-info.js",
    "start:mock": "yarn stop && node --inspect tests/runServer-mockExternal.js",
    "start:mockApi": "node tests/mockApiServer.js",
    "start:prod": "node scripts/runServer.js",
    "stop": "forever stopall",
    "tail": "tail -f ./*.log",
    "test": "npm-run-all --parallel lint:base test:unit",
    "test:coverage": "yarn run test:unit -- --coverage --runInBand",
    "test:integration": "NODE_ENV=test jest --config jest.int.config.json",
    "test:base": "NODE_ENV=test jest --config jest.config.json",
    "test:unit": "yarn run test:base -- --silent",
    "test:watch": "yarn run test:base -- --watch --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetup/web-platform-starter.git"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "flatpickr": "2.5.8",
    "html-entities": "1.2.1",
    "inert": "4.2.0",
    "meetup-web-components": "1.0.860",
    "meetup-web-platform": "2.4.1308",
    "node-fetch": "1.6.3",
    "react": "15.5.4",
    "react-helmet": "5.0.3",
    "react-hot-loader": "next",
    "react-intl": "2.2.3",
    "react-redux": "5.0.4",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0",
    "rxjs": "5.3.0",
    "swarm-icons": "0.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "19.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "coveralls": "2.13.0",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-config-prettier": "1.7.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-meetup": "git+https://github.com/meetup/eslint-plugin-meetup.git",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.11.1",
    "flow-bin": "0.44.2",
    "forever": "0.15.3",
    "gettext-parser": "1.2.2",
    "glob": "7.1.1",
    "husky": "0.13.3",
    "inquirer": "3.0.6",
    "jest-cli": "19.0.2",
    "lint-staged": "3.4.1",
    "loader-utils": "1.1.0",
    "meetup-web-mocks": "1.0.183",
    "mkdirp": "0.5.1",
    "node-sass": "4.5.2",
    "npm-run-all": "4.0.2",
    "prettier": "1.2.2",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "swarm-sasstools": "1.2.11",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.4",
    "webpack-node-externals": "1.5.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --single-quote --use-tabs --trailing-comma es5",
      "eslint",
      "git add"
    ]
  }
}
