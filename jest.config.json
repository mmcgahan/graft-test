{
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/{node_modules,build,coverage,scripts,templates,tests,flow}/**",
    "!util/**"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "setupFiles": [
    "<rootDir>/util/jestSystemShim.js"
  ],
  "moduleNameMapper": {
    "^(file-loader[!?].+|raw-loader[!?].+|base64-image-loader[!?].+)": "<rootDir>/__mocks__/emptyString.js",
    "^src(.*)": "<rootDir>/src$1"
  },
  "moduleFileExtensions": [
    "js",
    "jsx",
    "node",
    "json"
  ],
  "testRegex": "(src|util|scripts)/.*\\.test\\.jsx?$",
  "transformIgnorePatterns": [
    "node_modules(?!\/meetup-web-components)"
  ]
}
