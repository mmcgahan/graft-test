{
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/{node_modules,build,coverage,scripts,templates}/**",
    "!util/**"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "setupFiles": [
    "<rootDir>/util/jestSystemShim.js"
  ],
  "moduleNameMapper": {
    "^(file-loader[!?].+|trns$|raw-loader[!?].+)": "EmptyModule"
  },
  "moduleFileExtensions": [
    "js",
    "jsx",
    "node",
    "json"
  ],
  "testRegex": "(src|util|scripts)/.*\\.test\\.jsx?$",
  "transformIgnorePatterns": [
    "node_modules(?!\/meetup-web-components)"
  ]
}

